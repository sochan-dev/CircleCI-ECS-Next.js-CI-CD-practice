{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  "rules": {
    "eqeqeq": "error",
    "arrow-parens": "error",
    "arrow-spacing": "error",
    "react-hooks/exhaustive-deps": "off",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}